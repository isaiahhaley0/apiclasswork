<div class="form-data">
    <form action="/subscribers/create" method="POST">
        <label for = "txtName">
            Name
        </label>
        <input type="text" name="name" id="txtName" autofocus required>
        <label for = "txtEmail">
            Email
        </label>
        <input type="email" name="email" id="txtEmail" required>
        <label for = "txtZipcode">
            zipCode
        </label>
        <input type="text" name="zipCode" id="txtZipcode" required>
        <button type="submit">Create</button>
    </form>
</div>