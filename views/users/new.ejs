<div class="form-data">
    <form action="/users/create" method="POST">
        <label for = "txtFirstName">
            First Name
        </label>
        <input type="text" name="firstName" id="txtFirstName" autofocus required>
        <label for = "txtLastName">
            Last Name
        </label>
        <input type="text" name="lastName" id="txtLastName" autofocus required>
        <label for = "txtEmail">
            Email
        </label>
        <input type="email" name="email" id="txtEmail" required>
        <label for = "txtZipcode">
            zipCode
        </label>
        <input type="text" name="zipCode" id="txtZipcode" required>
        <label for = "txtPassword">
            password
        </label>
        <input type="text" name="password" id="txtPassword" required>
        <button type="submit">Create</button>
    </form>
</div>